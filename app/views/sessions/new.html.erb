<h1>Log in</h1>

<%= form_with url: sessions_path do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  
  <div>
    <%= form.submit "Log in" %>
  </div>
<% end %>